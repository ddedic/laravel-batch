{
    "name": "wfeller/laravel-batch",
    "description": "Insert or update models in batch, while still firing model events.",
    "keywords": ["laravel", "batch", "queue"],
    "homepage": "https://github.com/wfeller/laravel-batch",
    "license": "MIT",
    "authors": [
        {
            "name": "William",
            "homepage": "https://github.com/wfeller"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/bus": "~5.7.0|~5.8.0",
        "illuminate/contracts": "~5.7.0|~5.8.0",
        "illuminate/database": "~5.7.0|~5.8.0",
        "illuminate/support": "~5.7.0|~5.8.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9",
        "opensoft/doctrine-postgres-types": "^1.3",
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "WF\\Batch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WF\\Batch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
